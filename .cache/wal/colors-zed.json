{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#070f1290",
                "editor.background": "#070f1290",
                "editor.foreground": "#c1c3c3",
                "text": "#c1c3c3",
                "text.muted": "#c1c3c370",
                "text.ignored": "#c1c3c340",
                "text.placeholder": "#c1c3c350",
                "ignored": "#c1c3c330",
                "element.hover": "#c1c3c330",
                "ghost_element.hover": "#063C4430",
                "ghost_element.selected": "#063C4430",
                "ghost_element.active": "#063C4460",
                "border": "#070f12",
                "editor.highlighted_line.background": "#063C4410",
                "editor.active_line.background": "#063C4410",
                "panel.background": "#070f1290",
                "title_bar.background": "#070f1290",
                "title_bar.inactive_background": "#070f1290",
                "status_bar.background": "#070f1290",
                "drop_target.background": "#070f1290",
                "elevated_surface.background": "#070f12",
                "toolbar.background": "#070f1290",
                "tab_bar.background": "#070f1290",
                "tab.inactive_background": "#070f1290",
                "tab.active_background": "#063C4430",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#070f12",
                "scrollbar.thumb.background": "#063C44",
                "editor.gutter.background": "#070f1290",
                "terminal.background": "#070f1210",
                "terminal.foreground": "#c1c3c3",
                "terminal.dim_foreground": "#c1c3c3",
                "terminal.bright_foreground": "#c1c3c3",
                "terminal.ansi.black": "#070f12",
                "terminal.ansi.red": "#063C44",
                "terminal.ansi.green": "#0C4E4B",
                "terminal.ansi.yellow": "#136759",
                "terminal.ansi.blue": "#119B5C",
                "terminal.ansi.magenta": "#23C253",
                "terminal.ansi.cyan": "#8AE45E",
                "terminal.ansi.white": "#c1c3c3",
                "terminal.ansi.bright_black": "#56676a",
                "terminal.ansi.bright_red": "#063C44",
                "terminal.ansi.bright_green": "#0C4E4B",
                "terminal.ansi.bright_yellow": "#136759",
                "terminal.ansi.bright_blue": "#119B5C",
                "terminal.ansi.bright_magenta": "#23C253",
                "terminal.ansi.bright_cyan": "#8AE45E",
                "terminal.ansi.bright_white": "#c1c3c3",
                "modified": "#063C44",
                "syntax": {
                    "attribute": {
                        "color": "#063C44"
                    },
                    "boolean": {
                      "color": "#0C4E4B"
                    },
                    "comment": {
                      "color": "#56676a"
                    },
                    "comment.doc": {
                      "color": "#56676a"
                    },
                    "constant": {
                      "color": "#0C4E4B"
                    },
                    "function": {
                      "color": "#8AE45E"
                    },
                    "keyword": {
                      "color": "#119B5C"
                    },
                    "number": {
                      "color": "#23C253"
                    },
                    "operator": {
                      "color": "#119B5C"
                    },
                    "string": {
                      "color": "#063C44"
                    },
                    "variable": {
                      "color": "#0C4E4B"
                    }
                },
                "players": [
                  {
                    "cursor": "#063C44",
                    "background": "#070f12",
                    "selection": "#063C4430"
                  }
                ]
            }
        }
    ]
}
