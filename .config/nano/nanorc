set constantshow
set mouse
#set smooth
set autoindent
set linenumbers
set softwrap
set atblanks
set tabsize 4
set tabstospaces

bind ^S savefile main
bind ^Q exit main
bind ^Z undo main
bind ^Y redo main
bind ^F whereis main
include "/usr/share/nano-syntax-highlighting/*.nanorc"
include "/usr/share/nano/*.nanorc"
